{
    "expo": {
        "name": "BrowserReader",
        "slug": "BrowserReader",
        "version": "1.2.0",
        "orientation": "portrait",
        "icon": "./assets/images/icon.png",
        "scheme": "app",
        "userInterfaceStyle": "automatic",
        "newArchEnabled": true,
        "ios": {
            "supportsTablet": false
        },
        "android": {
            "edgeToEdgeEnabled": true,
            "predictiveBackGestureEnabled": false,
            "permissions": ["android.permission.ACCESS_FINE_LOCATION"],
            "package": "com.anonymous.BrowserReader"
        },
        "web": {
            "output": "static",
            "favicon": "./assets/images/icon.png"
        },
        "plugins": [
            "expo-router",
            [
                "expo-splash-screen",
                {
                    "image": "./assets/images/icon.png",
                    "imageWidth": 200,
                    "resizeMode": "contain",
                    "backgroundColor": "#ffffff",
                    "dark": {
                        "backgroundColor": "#000000"
                    }
                }
            ],
            "expo-font",
            "expo-navigation-bar"
        ],
        "experiments": {
            "typedRoutes": true,
            "reactCompiler": true
        },
        "extra": {
            "router": {},
            "eas": {
                "projectId": "0990d148-2301-4265-a387-14104c167bc4"
            }
        }
    }
}
